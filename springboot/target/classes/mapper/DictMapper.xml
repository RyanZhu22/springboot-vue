<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboot_restful.mapper.DictMapper">
    <select id="findIcon" resultType="com.example.springboot_restful.entity.Dict">
        SELECT * FROM sys_dict
        WHERE type = "icon"
    </select>
</mapper>